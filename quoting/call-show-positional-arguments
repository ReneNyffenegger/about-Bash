#
# vim: ft=sh
#

echo calling show-positional-arguments with \$*
./show-positional-arguments $*

echo calling show-positional-arguments with "\@*"
./show-positional-arguments "$@"

# https://stackoverflow.com/a/743577/180275
echo calling show-positional-arguments with \${1+\$@}
./show-positional-arguments ${1+$@}
